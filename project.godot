config_version=4

[application]

config/name="The Monad"
config/description="I am the Monad, the Ultimate, the All, the One. All that exists comes from me."
run/main_scene="res://src/menu.tscn"
config/icon="res://monad.png"
config/windows_native_icon="res://monad.ico"

[autoload]

Music="*res://src/music/music.tscn"
Levels="*res://data/Levels.tscn"
PlayerData="*res://src/player/PlayerData.tscn"

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

